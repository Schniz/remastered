{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["vite/client", "@types/jest"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "react-router": [".remaster/dist/react-router-pkgs/react-router"],
      "react-router-dom": [".remaster/dist/react-router-pkgs/react-router-dom"],
      "react-router-dom/server": [".remaster/dist/react-router-pkgs/react-router-dom/server"],
      "@remaster/core/*": [".remaster/*"]
    }
  },
  "include": ["./src", "./app"]
}
