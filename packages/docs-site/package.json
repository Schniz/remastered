{
  "name": "@remastered/docs-site",
  "version": "0.1.13",
  "private": true,
  "author": "Gal Schlezinger <gal@spitfire.co.il>",
  "main": "lib/docs-site.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Schniz/remastered.git"
  },
  "workspaces": {
    "nohoist": [
      "**/shiki",
      "**/shiki/**"
    ]
  },
  "scripts": {
    "dev": "remastered dev",
    "start": "remastered serve",
    "build": "REMASTERED_DEBUG_MANIFEST=true remastered build",
    "vercel-build": "lerna run build --scope=@remastered/docs-site --include-dependencies && remastered-vercel postbuild && remastered-vercel export"
  },
  "bugs": {
    "url": "https://github.com/Schniz/remastered/issues"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.1",
    "@remastered/vercel": "^0.1.13",
    "@stefanprobst/remark-shiki": "^0.1.2",
    "@tabler/icons": "^1.41.2",
    "@tailwindcss/typography": "^0.4.0",
    "front-matter": "^4.0.2",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "remark": "^13.0.0",
    "remark-frontmatter": "^3.0.0",
    "remark-gfm": "^1.0.0",
    "remark-html": "^13.0.1",
    "remastered": "^0.1.13",
    "shiki": "^0.9.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.15",
    "prettier": "^2.3.0",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.4"
  }
}
