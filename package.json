{
  "name": "remastered",
  "version": "0.0.0",
  "scripts": {
    "dev": "node -r @swc-node/register src/server",
    "build": "tsc && yarn build:client && yarn build:server && node ./scripts/generateNextHelpers.js",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.tsx",
    "start": "node -r @swc-node/register src/server",
    "serve": "node -r @swc-node/register src/server",
    "vercel-build": "yarn build && cp dist/server/template.js dist/server/entry-server.js dist/client/ssr-manifest.json ./api/"
  },
  "dependencies": {
    "@swc-node/register": "^1.2.1",
    "fastify": "^3.15.1",
    "fastify-express": "^0.3.2",
    "fastify-static": "^4.0.1",
    "history": "^5.0.0",
    "lodash": "^4.17.21",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0"
  },
  "devDependencies": {
    "@swc-node/jest": "^1.2.1",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2",
    "vite": "^2.2.3"
  }
}
